PATH:Workspace/Attack Dummy/DummyM1Attack
CLASS:Script
--SOURCE--  -  Edit
  20:22:32.092  -- DummyM1Attack.server
local RS         = game:GetService("ReplicatedStorage")
local Players    = game:GetService("Players")

local Constants         = require(RS.Shared.Modules.Constants)
local AnimatorUtil      = require(RS.Shared.Modules.AnimatorUtil)
local HitboxPartService = require(game.ServerScriptService.Services.HitboxPartService)
local BlockService      = require(game.ServerScriptService.Services.BlockService)
local CombatService     = require(game.ServerScriptService.Services.CombatService)
local VFXService        = require(game.ServerScriptService.Services.VFXService)

local Model = script.Parent
local Hum   = Model:FindFirstChildOfClass("Humanoid")
local Root  = Model:FindFirstChild("HumanoidRootPart")
if not (Hum and Root) then warn("[Dummy] Humanoid or HRP missing"); return end

local ANIMS = {"M1_1","M1_2","M1_3","M1_4"}
local HIT_MARKER     = Constants.Melee.HitMarkerName or "HIT"
local STEP_DELAY     = 0.2
local SERIES_PAUSE   = 2.0
local MARKER_TIMEOUT = 2.0
local PARRY_STUN     = Constants.Block.AttackerStun or 3.0       -- теперь длинный
local HEAVY_STUN     = Constants.Block.HeavyBlockStun or 0.4     -- теперь короткий

local stunUntil = 0
local baseWS, baseJP = Hum.WalkSpeed, Hum.JumpPower
local currentTrack

local function now() return os.clock() end
local function isStunned() return now() < stunUntil end
local function restoreMovement() if Hum and Hum.Parent then Hum.WalkSpeed = baseWS; Hum.JumpPower = baseJP end end
local function startStun(duration)
	local endAt = now() + duration
	if endAt > stunUntil then stunUntil = endAt end
	if Hum and Hum.Parent then Hum.WalkSpeed, Hum.JumpPower = 0, 0 end
	if currentTrack then pcall(function() currentTrack:Stop(0) end); currentTrack = nil end
end
local function finishStunAndRestore()
	local endAt = stunUntil
	task.spawn(function()
		repeat task.wait(0.05) until now() >= endAt or not Hum or not Hum.Parent
		if Hum and Hum.Parent and now() >= stunUntil then restoreMovement() end
	end)
end

local function faceNearestPlayer()
	if isStunned() then return end
	local nearest, dist = nil, math.huge
	for _, pl in ipairs(Players:GetPlayers()) do
		local ch = pl.Character
		local hrp = ch and ch:FindFirstChild("HumanoidRootPart")
		if hrp then
			local d = (hrp.Position - Root.Position).Magnitude
			if d < dist then nearest, dist = ch, d end
		end
	end
	if nearest then
		local here = Root.Position
		local look = Vector3.new(nearest.HumanoidRootPart.Position.X, here.Y, nearest.HumanoidRootPart.Position.Z)
		Root.CFrame = CFrame.lookAt(here, look)
	end
end

local function doHit(step)
	if isStunned() then return end
	local dmg = Constants.Melee.Damage[step] or 1

	local found = HitboxPartService:Query(Model, Root.CFrame, {
		size     = Constants.Melee.HitboxSize,
		offset   = Constants.Melee.HitboxOffset,
		yOffset  = Constants.Melee.HitboxYOffset,
		backPad  = Constants.Melee.HitboxBackPad,
		exclude  = { Model },
		debug    = Constants.Melee.HitboxDebug,
		debugLife= Constants.Melee.HitboxDebugLife,
	})

	for _, t in ipairs(found) do
		if t.player then
			local eval = BlockService:EvaluateHit(nil, t.player, Root, t.root)
			if eval.type == "parry" then
				startStun(PARRY_STUN); finishStunAndRestore()
			elseif eval.type == "block" then
				if step == (Constants.Melee.ComboHits or 4) then startStun(HEAVY_STUN); finishStunAndRestore() end
			elseif eval.type == "backhit" then
				CombatService:ApplyDamageToPlayer(nil, t.player, dmg)
				BlockService:GuardBreak(t.player)
			else
				CombatService:ApplyDamageToPlayer(nil, t.player, dmg)
			end
		else
			CombatService:ApplyDamageToHumanoid(nil, t.humanoid, dmg)
		end

		VFXService:PlayOnCharacter(t.model, Constants.Melee.HitVFXName, {
			duration  = Constants.Melee.HitVFXLifetime,
			emitCount = Constants.Melee.HitVFXEmitCount,
			throttle  = 0.08,
		})
	end
end

local function playStep(step)
	if isStunned() then return false end
	local name = ANIMS[step]
	local track = AnimatorUtil.Play(Model, name, { folder="Common", speed=1, fade=0.02, weight=1, looped=false })
	currentTrack = track
	local fired = false
	local conn
	if track and track.GetMarkerReachedSignal then
		conn = track:GetMarkerReachedSignal(HIT_MARKER):Connect(function()
			fired = true; doHit(step)
		end)
	end
	local t0 = now()
	while track and track.IsPlaying and (now() - t0) < MARKER_TIMEOUT do
		if isStunned() then pcall(function() track:Stop(0) end); break end
		task.wait(0.03)
	end
	if conn then conn:Disconnect() end
	currentTrack = nil
	if (not fired) and (not isStunned()) then doHit(step) end
	return not isStunned()
end

task.spawn(function()
	while Model and Model.Parent do
		if not isStunned() then
			faceNearestPlayer()
			for i = 1, #ANIMS do
				if isStunned() then break end
				local ok = playStep(i)
				if isStunned() then break end
				if ok then task.wait(STEP_DELAY) end
			end
			task.wait(SERIES_PAUSE)
			if not isStunned() then restoreMovement() end
		else
			task.wait(0.05)
			if not isStunned() then restoreMovement(); task.wait(0.2) end
		end
	end
end)

print("[Dummy] Attack loop started (stun-aware, pauses, cooldowned parry punish)")  -  Edit
