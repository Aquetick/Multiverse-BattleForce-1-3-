--SOURCE--  -  Edit
  20:22:32.092  local TweenService = game:GetService("TweenService")
local hum = script.Parent.Parent.Parent.Parent.Parent.Humanoid
script.Parent.Text = math.floor((hum.Health/hum.MaxHealth)*100).."%"
script.Parent.TextColor3 = Color3.new(1, 1, 1)
local duration = 0.7 

local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear)

function colorcahnge(endColor)
	local tween = TweenService:Create(script.Parent, tweenInfo, { TextColor3 = endColor })
	tween:Play()
end

-- Start the tween
local currhealth = nil

hum.HealthChanged:Connect(function(health)
	script.Parent.Text = tostring(math.floor((health/hum.MaxHealth) * 100)).."%"
	currhealth = math.floor((health/hum.MaxHealth) * 100)
	
	if currhealth >= 75 then
		colorcahnge(Color3.new(1, 1, 1))
	elseif currhealth < 75 and currhealth >= 25 then
		colorcahnge(Color3.new(0.898039, 0.898039, 0.898039))
	else
		colorcahnge(Color3.new(0.709804, 0.709804, 0.709804))
	end
end)
